	svg(class="logo-icon" width="70" height="70" viewbox="0 0 200 200" xmlns="http://www.w3.org/2000/svg")
		image.logo-icon__sofa(href="img/logo-figure.png" x="70" y="20" width="60" height="47" preserveAspectRatio="xMidYMid meet")
